{
  "name": "meta-y",
  "displayName": "MetaY.ai",
  "version": "0.0.1",
  "description": "MetaY.ai mining extension",
  "author": "Blockfish",
  "scripts": {
    "dev": "plasmo dev --verbose",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "alias": {
    "react-native": "react-native-web"
  },
  "dependencies": {
    "@plasmohq/redux-persist": "6.1.0",
    "@plasmohq/storage": "^1.12.0",
    "@reduxjs/toolkit": "2.0.1",
    "@swc/core": "1.5.7",
    "blueimp-md5": "^2.19.0",
    "plasmo": "^0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.30.3",
    "redux": "5.0.1",
    "redux-persist-webextension-storage": "1.0.2",
    "redux-thunk": "3.1.0",
    "web3": "^4.8.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "plasmo": {
    "permissions": [
      "contextMenus",
      "notifications",
      "storage",
      "tabs",
      "activeTab",
      "scripting",
      "cookies",
      "<all_urls>"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.ts"
    },
    "content_scripts": [
      {
        "js": [
          "./pagenote-5.4.7/pagenote.js",
          "./libs/underscore-1.8.3.js",
          "./libs/md5-browser.js",
          "./libs/Readability.js",
          "./libs/html2md-1.0.js",
          "./libs/html2canvas.js",
          "./libs/qrcode.js",
          "./libs/lodash.js",
          "./tools.js",
          "./economistCom-unlocked/main.js"
        ],
        "css": [
          "./pagenote-5.4.7/pagenote.css",
          "./feynman-book.css",
          "./feynman.css",
          "./economistCom-unlocked/extenCard.css"
        ],
        "matches": [
          "https://*/*",
          "http://*/*"
        ],
        "run_at": "document_end"
      }
    ]
  }
}
