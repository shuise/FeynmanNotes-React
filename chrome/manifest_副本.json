{
    "name": "Feynman 笔记 Beta",
    "description": "笔记并不是思考的过程，笔记就是思考本身。",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
        "128": "./logo.png"
    },
    "browser_action": {
        "default_icon": "./logo.png",
        "default_title": "Feynman 笔记",
        "default_popup": "feynman.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [{
        "js": [
            "./pagenote-5.4.7/pagenote.js",
            "./libs/underscore-1.8.3.js",
            "./libs/vue-2.6.10.js",
            "./libs/md5-browser.js",
            "./libs/Readability.js",
            "./libs/html2md-1.0.js",
            "./libs/html2canvas.js",
            "./libs/qrcode.js",
            "./libs/lodash.js",
            "./tools.js",
            "./feynman.js"
        ],
        "css": [
            "./pagenote-5.4.7/pagenote.css",
            "./feynman.css"
        ],
        "matches": [
            "https://*/*",
            "http://*/*"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "contextMenus",
        "notifications",
        "storage",
        "tabs",
        "activeTab",
        "scripting",
        "browsingData",
        "cookies",
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    }
}